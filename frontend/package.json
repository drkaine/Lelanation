{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "eslint . --fix && prettier --write src/ && vue-tsc --build --force &&  npm run test:unit && run-p type-check \"build-only {@}\" --",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" -- && npm run sitemap",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "sitemap": "node scripts/generate-sitemap.js"
  },
  "dependencies": {
    "@iconify/vue": "^4.3.0",
    "@vue-office/excel": "^1.7.14",
    "axios": "^1.9.0",
    "chart.js": "^4.4.7",
    "dom-to-image-more": "^3.5.0",
    "esbuild": ">=0.25.0",
    "fods": "^2.2.1",
    "html2canvas": "^1.4.1",
    "pinia": "^2.2.4",
    "uuid": "^11.0.3",
    "vue": "^3.5.12",
    "vue-chartjs": "^5.3.2",
    "vue-i18n": "^12.0.0-alpha.2",
    "vue-router": "^4.4.5",
    "vue-uuid": "^3.0.0",
    "vuedraggable": "^2.24.3",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/chart.js": "^2.9.41",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.10",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "esbuild": ">=0.25.0",
    "eslint": "^9.12.0",
    "eslint-plugin-playwright": "^1.7.0",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "terser": "^5.39.0",
    "typescript": "~5.5.4",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vitest": ">=2.1.9",
    "vue-tsc": "^2.1.6"
  }
}
