{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "npm run lint && npm run format && npm run type-check && npm run test && npm run build",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" -- && npm run sitemap && npm run seo:validate",
    "preview": "vite preview",
    "test": "npm run test:unit ",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "sitemap": "tsx scripts/generate-sitemap.ts",
    "seo:audit": "tsx -e \"import('./src/utils/seoAudit.ts').then(m => m.SEOAuditor.logReport())\"",
    "seo:validate": "tsx -e \"import('./src/utils/sitemapGenerator.ts').then(m => { const issues = m.validateSitemapForNoIndex(m.generateLelanationSitemap(true)); if(issues.length > 0) { console.error('❌ Sitemap validation failed:', issues); process.exit(1); } else { console.log('✅ Sitemap validation passed'); } })\"",
    "seo:audit-sitemap": "tsx -e \"import('./src/utils/sitemapGenerator.ts').then(m => { const sitemap = m.generateLelanationSitemap(true); const audit = m.auditSitemapForNoIndexIssues(sitemap); console.log(audit.summary); if(audit.noIndexUrls.length > 0) process.exit(1); })\"",
    "seo:audit-noindex": "tsx -e \"import('./src/utils/noindexAudit.ts').then(m => m.NoIndexAuditor.logAuditReport())\"",
    "seo:headings-check": "tsx scripts/seo-headings-check.ts",
    "seo:final-check": "tsx scripts/seo-final-check.ts",
    "security:csp-check": "tsx -e \"import('./src/utils/cspValidator.ts').then(m => m.CSPValidator.logSecurityReport())\"",
    "security:csp-nginx": "tsx -e \"import('./src/utils/cspValidator.ts').then(m => console.log(m.CSPValidator.getRecommendedCSPForNginx()))\"",
    "security:validate": "tsx scripts/security-validation.ts",
    "seo:check": "npm run seo:validate && npm run seo:audit-sitemap && npm run seo:audit-noindex && npm run seo:audit",
    "build:seo": "npm run seo:validate && npm run build"
  },
  "dependencies": {
    "@iconify/vue": "^4.3.0",
    "@vue-office/excel": "^1.7.14",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.9.0",
    "chart.js": "^4.4.7",
    "dom-to-image-more": "^3.5.0",
    "esbuild": ">=0.25.0",
    "fods": "^2.2.1",
    "html2canvas": "^1.4.1",
    "pinia": "^2.2.4",
    "uuid": "^11.0.3",
    "vue": "^3.5.12",
    "vue-chartjs": "^5.3.2",
    "vue-i18n": "^12.0.0-alpha.2",
    "vue-router": "^4.4.5",
    "vue-uuid": "^3.0.0",
    "vuedraggable": "^2.24.3",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/chart.js": "^2.9.41",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.10",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "esbuild": ">=0.25.0",
    "eslint": "^9.12.0",
    "eslint-plugin-playwright": "^1.7.0",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "terser": "^5.39.0",
    "tsx": "^4.19.4",
    "typescript": "~5.5.4",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vitest": ">=2.1.9",
    "vue-tsc": "^2.1.6"
  }
}
